@model IEnumerable<Register>

@{
    ViewData["Title"] = "LoginPage";
}


<div class="row pt-3">
  
    <div class="col-6 text-end">
        <a asp-controller="Home" asp-action="Login" class="btn btn-info">Logout</a>
    </div>
</div>



<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col"></th>
            <th scope="col"> Name</th>
            <th scope="col"> Age</th>
            <th scope="col"> Address</th>
            <th scope="col"> Course</th>
            <th scope="col">Email Address</th>
            <th scope="col"> Password</th>
            <th scope="col">Phone Number</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var u in Model)
        {
            <tr class="table-dark">
                <th scope="row"></th>
                <td>@u.Name</td>
                <td>@u.Age</td>
                <td>@u.Address</td>
                <td>@u.Course</td>
                <td>@u.Email</td>
                <td>@u.Password</td>
                <td>@u.PhoneNumber</td>
                <td>
                    <a asp-route-id="@u.Id" asp-controller="Home" asp-action="Edit" class="btn btn-info"><i class="bi bi-pencil-square"></i></a>
                    <a asp-route-id="@u.Id" asp-controller="Home" asp-action="Delete" class="btn btn-info"><i class="bi bi-trash"></i> </a>
                </td>
            </tr>
        }


    </tbody>
</table>



<a asp-controller="Home" asp-action="Login" class="btn btn-info">Back</a>
<Partial name="_Notification" />


